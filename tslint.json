{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single", "avoid-escape"],
      "no-console": [false],
      "ordered-imports": [false],
      "trailing-comma": [false],
      "no-consecutive-blank-lines": [false],
      "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
